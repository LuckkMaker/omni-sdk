{
    "cmake.generator": "Unix Makefiles",
    "cmake.clearOutputBeforeBuild": true,
    "cmake.buildTask": true,
    "cmake.environment": {
        "CLICOLOR_FORCE": "1"
    },
    "ceedlingExplorer.projectPath": "test",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope" : "markup.other.log.error",
                "settings": { "foreground": "#C50F1F" }
            },
            {
                "scope" : "markup.other.log.warn",
                "settings": { "foreground": "#C19C00" }
            },
            {
                "scope" : "markup.other.log.info",
                "settings": { "foreground": "#3A96DD" }
            },
            {
                "scope" : "markup.other.log.debug",
                "settings": { "foreground": "#888585" }
            },
            {
                "scope" : "markup.other.log.highlight",
                "settings": { "foreground": "#13A10E" }
            },
        ]
    },
    "files.associations": {
        "*.cjson": "jsonc",
        "*.wxss": "css",
        "*.wxs": "javascript",
        "main.h": "c"
    },
}